<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.rollcall_10">


    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/rollcall_app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".MainActivity">

        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>

    </activity>


        <activity android:name=".BLE_MainActivity">

        </activity>

        <activity android:name=".ManualAdd_BLE_MainActivity">

        </activity>


        <activity android:name=".Activity_BTLE_Services" />


        <activity android:name=".Watch_List" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.administrator.rollcall_10.BLE_MainActivity" />
        <service
            android:name=".Service_BTLE_GATT"
            android:enabled="true"
            android:exported="true">

        </service>



            <!-- Parent activity meta-data to support 4.0 and lower -->



    </application>

</manifest>